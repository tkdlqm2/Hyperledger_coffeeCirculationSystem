s=\"control\">\n                <b-switch v-model=\"isStriped\">Striped</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"isNarrowed\">Narrowed</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"isHoverable\">Hoverable</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"isFocusable\">Focusable</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"isLoading\">Loading state</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"isEmpty\">Empty</b-switch>\n            </div>\n            <div class=\"control\">\n                <b-switch v-model=\"hasMobileCards\">Mobile cards <small>(collapsed rows)</small></b-switch>\n            </div>\n        </b-field> -->\n        <h3 class=\"title\">이력 조회</h3>\n        <b-table\n            :data=\"isEmpty ? [] : data\"\n            :bordered=\"isBordered\"\n            :striped=\"isStriped\"\n            :narrowed=\"isNarrowed\"\n            :hoverable=\"isHoverable\"\n            :loading=\"isLoading\"\n            :focusable=\"isFocusable\"\n            :mobile-cards=\"hasMobileCards\">\n\n            <template slot-scope=\"props\">\n                <b-table-column field=\"id\" label=\"ID\" width=\"40\" numeric>\n                    {{ props.row.id }}\n                </b-table-column>\n\n                <b-table-column field=\"first_name\" label=\"First Name\">\n                    {{ props.row.first_name }}\n                </b-table-column>\n\n                <b-table-column field=\"last_name\" label=\"Last Name\">\n                    {{ props.row.last_name }}\n                </b-table-column>\n\n                <b-table-column field=\"date\" label=\"Date\" centered>\n                    <span class=\"tag is-success\">\n                        {{ new Date(props.row.date).toLocaleDateString() }}\n                    </span>\n                </b-table-column>\n\n                <b-table-column label=\"Gender\">\n                    <span>\n                        <b-icon pack=\"fas\"\n                            :icon=\"props.row.gender === 'Male' ? 'mars' : 'venus'\">\n                        </b-icon>\n                        {{ props.row.gender }}\n                    </span>\n                </b-table-column>\n            </template>\n\n            <template slot=\"empty\">\n                <section class=\"section\">\n                    <div class=\"content has-text-grey has-text-centered\">\n                        <p>\n                            <b-icon\n                                icon=\"emoticon-sad\"\n                                size=\"is-large\">\n                            </b-icon>\n                        </p>\n                        <p>Nothing here.</p>\n                    </div>\n                </section>\n            </template>\n        </b-table>\n    </section>\n</template>\n\n<script>\nimport CommonService from '../../../api/common/commonService'\nexport default {\n\n    data() {\n        const data = [\n            { 'id': 1, 'first_name': 'Jesse', 'last_name': 'Simmons', 'date': '2016/10/15 13:43:27', 'gender': 'Male' },\n            { 'id': 2, 'first_name': 'John', 'last_name': 'Jacobs', 'date': '2016/12/15 06:00:53', 'gender': 'Male' },\n            { 'id': 3, 'first_name': 'Tina', 'last_name': 'Gilbert', 'date': '2016/04/26 06:26:28', 'gender': 'Female' },\n            { 'id': 4, 'first_name': 'Clarence', 'last_name': 'Flores', 'date': '2016/04/10 10:28:46', 'gender': 'Male' },\n            { 'id': 5, 'first_name': 'Anne', 'last_name': 'Lee', 'date': '2016/12/06 14:38:38', 'gender': 'Female' }\n        ]\n        \n        return {\n            data,\n            isEmpty: false,\n            isBordered: false,\n            isStriped: false,\n            isNarrowed: false,\n            isHoverable: false,\n            isFocusable: false,\n            isLoading: false,\n            hasMobileCards: true\n        }\n    },\n\n    mounted() {\n        CommonService.getHistory();\n    }\n}\n</script>"],"sourceRoot":"src/views/main/common"}]}